{
  "name": "driver-backend",
  "version": "1.0.0",
  "description": "Driver backend service for transit app",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "db:seed": "ts-node src/seed.ts",
    "docker:build": "docker build -t driver-backend .",
    "docker:run": "docker run -p 3000:3000 driver-backend",
    "deploy:render": "npm run build && npm start",
    "health:check": "curl -f http://localhost:3000/health || exit 1"
